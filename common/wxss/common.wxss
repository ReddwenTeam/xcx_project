.nodisplay{ display: none !important; }
div { outline: 0 !important; -webkit-text-size-adjust: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0); position: relative; color: #333; }
.div-after { }
.div-after:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
.fl-l { float: left; }
.box { display: box; display: -webkit-box; }
.box-ac { box-align: center; -webkit-box-align: center; }
.box-as { box-align: stretch; -webkit-box-align: stretch; }
.box-ae { -webkit-box-align: end; box-align: end; }
.box-pc { box-pack: center; -webkit-box-pack: center; }
.box-pe { box-pack: end; -webkit-box-pack: end; }
.box-pj { -webkit-box-pack: justify; box-pack: justify; }
.box-ver { box-orient: vertical; -webkit-box-orient: vertical; }
.box-f1 { box-flex: 1; -webkit-box-flex: 1; }
.box-f2 { box-flex: 2; -webkit-box-flex: 2; }
.box-size { box-sizing: border-box; -webkit-box-sizing: border-box; }
.box-img { background-position: center; background-repeat: no-repeat; background-size: contain; }
.box-img0 { background-position: center; background-repeat: no-repeat; }
.box-img1 { background-position: center; background-repeat: no-repeat; background-size: cover; }
.box-img2 { background-position: center; background-repeat: no-repeat; background-size: 100% 100%; }
.box-ellipsis { text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; overflow: hidden; }
.ellipsis-1 {overflow : hidden; text-overflow: ellipsis; -webkit-line-clamp: 1; display: -webkit-box; -webkit-box-orient: vertical; }
.ellipsis-2 {overflow : hidden; text-overflow: ellipsis; -webkit-line-clamp: 2; display: -webkit-box; -webkit-box-orient: vertical; }
.ellipsis-3 {overflow : hidden; text-overflow: ellipsis; -webkit-line-clamp: 3; display: -webkit-box; -webkit-box-orient: vertical; }
.ta-center { text-align: center }
.ta-right { text-align: right }
.ta-justify { text-align: justify; }