@import "../../common/wxss/common";
/*slider*/
.slider-content { width: 100%; height: 350rpx; position: relative; }
/*info*/
.info-content { height: 136rpx; padding: 0 30rpx; }
.info-box { width: 200rpx; height: 60rpx; background-color: #f1ab44; line-height: 60rpx; text-align: center; border-radius: 20rpx }
.pos-head { height: 136rpx; position: relative }
.img-head { position: absolute; z-index: 2; left: 50%; width: 142rpx; height: 142rpx; margin: -71rpx 0 0 -71rpx; border: 5rpx solid #fff; border-radius: 50%; }
.bg-blue { width: 100%; height: 70rpx; background-color: #313680; padding: 10rpx 0; }
.width-5 { width: 50%; box-sizing: border-box; line-height: 70rpx; }
.width-5:first-child { border-right: dashed 1rpx #9fa0a0; padding: 0 30rpx }
.mg-r2 { margin-right: 20rpx; }
.mg-t3 { margin-top: 30rpx; }
.mg-t25 { margin-top: 25rpx; }
/*tab*/
.tab-content { padding: 5rpx 0 40rpx; }
.one-tab { width: 30%; background-color: #ffffff; margin-top: 25rpx; margin-left: 2.5%; padding: 20rpx 0; border-top: solid 1rpx rgba(0, 0, 0, .1); border-left: solid 1rpx rgba(0, 0, 0, .1); box-shadow: 2rpx 2rpx 5rpx rgba(0, 0, 0, .2); }
.img-tab { width: 90rpx; height: 90rpx; margin: 0 auto 15rpx; }
